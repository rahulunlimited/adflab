{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "rgadflau01devadf"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/LinkedServiceAzureDLGen2Storage')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "https://rgadflau01devdatalake.dfs.core.windows.net",
					"accountKey": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "LinkedServiceAzureKeyVault",
							"type": "LinkedServiceReference"
						},
						"secretName": "datalake-storage-key"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/LinkedServiceAzureKeyVault')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/LinkedServiceAzureFileStorage')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Connection for File Storage",
				"annotations": [],
				"type": "FileServer",
				"typeProperties": {
					"host": "\\\\rgadflau01devstorage.file.core.windows.net\\fshare",
					"userId": "AZURE\\rgadflau01devstorage",
					"password": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "LinkedServiceAzureKeyVault",
							"type": "LinkedServiceReference"
						},
						"secretName": "blob-storage-key"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/LinkedServiceAzureKeyVault')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/LinkedServiceAzureFunction')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureFunction",
				"typeProperties": {
					"functionAppUrl": "https://rgadflaudev01fnapp.azurewebsites.net",
					"functionKey": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "LinkedServiceAzureKeyVault",
							"type": "LinkedServiceReference"
						},
						"secretName": "azurefunction-fileservice-key"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/LinkedServiceAzureKeyVault')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/LinkedServiceAzureKeyVault')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureKeyVault",
				"typeProperties": {
					"baseUrl": "https://rgadflau01devkv.vault.azure.net/"
				}
			},
			"dependsOn": []
		}
	]
}