{
	"name": "LinkedServiceSourceAzureDatabase",
	"type": "Microsoft.DataFactory/factories/linkedservices",
	"properties": {
		"parameters": {
			"DBName": {
				"type": "String"
			}
		},
		"annotations": [],
		"type": "AzureSqlDatabase",
		"typeProperties": {
			"connectionString": "integrated security=False;data source=ralearnsql.database.windows.net;initial catalog=@{linkedService().DBName};user id=sqladmin;",
			"password": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "LinkedServiceAzureKeyVault",
					"type": "LinkedServiceReference"
				},
				"secretName": "db-source-azure-password"
			}
		}
	}
}